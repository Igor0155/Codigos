algoritmo "Idades dos Nadadores"

var
idade:real
inicio
// Seção de Comandos
escreva("Insira sua idade: ")
leia (idade)

se (idade <=10) entao
escreva ("Categoria Infantil.")
fimse

se (idade >10) e (idade<=17) entao
escreva  ("Categoria Juvenil.")
fimse

se (idade>17) entao
escreva ("Categoria Sênior.")

fimse
fimalgoritmo
